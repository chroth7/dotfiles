# PRETTIER

alias ptsx='npx prettier --write "**/*.tsx"'
alias pjsx='npx prettier --write "**/*.jsx"'
alias pts='npx prettier --write "**/*.ts"'
alias pjs='npx prettier --write "**/*.js"'

alias pall='npx prettier --write "**/*.{ts,tsx,js,jsx,md,css,html,json}"'
alias psrcall='npx prettier --write "src/**/*.{ts,tsx,js,jsx,md,css,html,json}"'
